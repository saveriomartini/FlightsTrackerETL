        <SQLTask:SqlTaskData
          SQLTask:Connection="{F765F8F3-E298-456C-93D8-14DD8199CEFF}"
          SQLTask:SqlStatementSource="IF NOT EXISTS (SELECT 1 FROM [dbo].[DimProfileReservation])&#xA;BEGIN&#xA;&#xA;-- Insertion de toutes les combinaisons possibles (48 lignes)&#xA;INSERT INTO [dbo].[DimProfileReservation] &#xA;(&#xA;    [SALES_CHANNEL],&#xA;    [TRIP_TYPE], &#xA;    [WANTS_EXTRA_BAGGAGE], &#xA;    [WANTS_PREFERRED_SEAT], &#xA;    [WANTS_IN_FLIGHT_MEALS]&#xA;)&#xA;SELECT &#xA;    Channel.Name,&#xA;    Trip.Name,&#xA;    Bag.Flag,&#xA;    Seat.Flag,&#xA;    Meal.Flag&#xA;FROM &#xA;    -- Set 1 : Canal de Vente (Valeurs trouvées dans ton Staging)&#xA;    (VALUES ('Internet'), ('Mobile')) AS Channel(Name)&#xA;CROSS JOIN &#xA;    -- Set 2 : Type de Voyage&#xA;    (VALUES ('CircleTrip'), ('OneWay'), ('RoundTrip')) AS Trip(Name)&#xA;CROSS JOIN &#xA;    -- Set 3 : Bagage (0 ou 1)&#xA;    (VALUES (0), (1)) AS Bag(Flag)&#xA;CROSS JOIN &#xA;    -- Set 4 : Siège (0 ou 1)&#xA;    (VALUES (0), (1)) AS Seat(Flag)&#xA;CROSS JOIN &#xA;    -- Set 5 : Repas (0 ou 1)&#xA;    (VALUES (0), (1)) AS Meal(Flag);&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
