          SQLTask:Connection="{F765F8F3-E298-456C-93D8-14DD8199CEFF}"
          SQLTask:SqlStatementSource="USE [FlightsTracker]&#xA;GO&#xA;&#xA;-- =============================================&#xA;-- ÉTAPE 0 : DÉVERROUILLER (Supprimer la FK sur la table de Faits)&#xA;-- =============================================&#xA;-- On vérifie si la contrainte existe, et si oui, on la supprime temporairement&#xA;IF EXISTS (SELECT * FROM sys.foreign_keys WHERE object_id = OBJECT_ID(N'[dbo].[FK_FaitsReservations_DimProfile]') AND parent_object_id = OBJECT_ID(N'[dbo].[FaitsReservations]'))&#xA;ALTER TABLE [dbo].[FaitsReservations] DROP CONSTRAINT [FK_FaitsReservations_DimProfile]&#xA;GO&#xA;&#xA;-- =============================================&#xA;-- ÉTAPE 1 : NETTOYAGE ET CRÉATION (DDL)&#xA;-- =============================================&#xA;&#xA;-- Maintenant que le lien est coupé, on peut DROP la table tranquille&#xA;IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'[dbo].[DimProfileReservation]') AND type in (N'U'))&#xA;DROP TABLE [dbo].[DimProfileReservation]&#xA;GO&#xA;&#xA;SET ANSI_NULLS ON&#xA;GO&#xA;SET QUOTED_IDENTIFIER ON&#xA;GO&#xA;&#xA;CREATE TABLE [dbo].[DimProfileReservation](&#xA;    [PROFILE_ID] [int] IDENTITY(1,1) NOT NULL,&#xA;    [SALES_CHANNEL] [varchar](50) NOT NULL,&#xA;    [TRIP_TYPE] [varchar](50) NOT NULL,&#xA;    [WANTS_EXTRA_BAGGAGE] [int] NOT NULL,&#xA;    [WANTS_PREFERRED_SEAT] [int] NOT NULL,&#xA;    [WANTS_IN_FLIGHT_MEALS] [int] NOT NULL,&#xA;    CONSTRAINT [PK_DimProfileReservation] PRIMARY KEY CLUSTERED &#xA;    (&#xA;        [PROFILE_ID] ASC&#xA;    )&#xA;) ON [PRIMARY]&#xA;GO&#xA;&#xA;-- =============================================&#xA;-- ÉTAPE 2 : PEUPLEMENT DES DONNÉES (DML)&#xA;-- =============================================&#xA;&#xA;INSERT INTO [dbo].[DimProfileReservation] &#xA;(&#xA;    [SALES_CHANNEL],&#xA;    [TRIP_TYPE], &#xA;    [WANTS_EXTRA_BAGGAGE], &#xA;    [WANTS_PREFERRED_SEAT], &#xA;    [WANTS_IN_FLIGHT_MEALS]&#xA;)&#xA;SELECT &#xA;    Channel.Name,&#xA;    Trip.Name,&#xA;    Bag.Flag,&#xA;    Seat.Flag,&#xA;    Meal.Flag&#xA;FROM &#xA;    (VALUES ('Internet'), ('Mobile'), ('Unknown')) AS Channel(Name)&#xA;CROSS JOIN &#xA;    (VALUES ('CircleTrip'), ('OneWay'), ('RoundTrip'), ('Unknown')) AS Trip(Name)&#xA;CROSS JOIN &#xA;    (VALUES (0), (1), (-1)) AS Bag(Flag)&#xA;CROSS JOIN &#xA;    (VALUES (0), (1), (-1)) AS Seat(Flag)&#xA;CROSS JOIN &#xA;    (VALUES (0), (1), (-1)) AS Meal(Flag);&#xA;GO&#xA;&#xA;-- =============================================&#xA;-- ÉTAPE 3 : REVERROUILLER (Remettre la FK sur la table de Faits)&#xA;-- =============================================&#xA;-- C'est très important de remettre la sécurité !&#xA;&#xA;ALTER TABLE [dbo].[FaitsReservations]  WITH CHECK ADD  CONSTRAINT [FK_FaitsReservations_DimProfile] FOREIGN KEY([FK_DIMPROFILERESERVATION])&#xA;REFERENCES [dbo].[DimProfileReservation] ([PROFILE_ID])&#xA;GO&#xA;&#xA;ALTER TABLE [dbo].[FaitsReservations] CHECK CONSTRAINT [FK_FaitsReservations_DimProfile]&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
